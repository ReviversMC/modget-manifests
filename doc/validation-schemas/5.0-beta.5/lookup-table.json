{
  "$id": "com.github.reviversmc.modget.manifests.schemas.spec5_0_beta_5.lookup-table",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "A representation of a Modget lookup table following the v5.0-beta.5 specification.",
  "type": "object",
  "properties": {
    "manifestSpecVersion": {
      "type": "string",
      "default": "\"5.0-beta.5\"",
      "const": "5.0-beta.5",
      "pattern": "^(0|[1-9]\\d*)((\\.(0|[1-9]\\d*))*)?(?:-((?:0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*)(?:\\.(?:0|[1-9]\\d*|\\d*[a-zA-Z-][0-9a-zA-Z-]*))*))?(?:\\+([0-9a-zA-Z-]+(?:\\.[0-9a-zA-Z-]+)*))?$",
      "title": "Current manifest specification"
    },
    "modIds": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "modId": {
            "type": "string",
            "pattern": "^([-_0-9a-zA-Z]+)$",
            "description": "The ID of the mod, see fabric.mod.json",
            "examples": [
              "sodium",
              "fdlink",
              "discerning_furnace"
            ]
          }
        },
        "required": [
          "modIds"
        ],
        "additionalProperties": false
      },
      "uniqueItems": true
    }
  },
  "required": [
    "manifestSpecVersion",
    "modIds"
  ],
  "additionalProperties": false
}
